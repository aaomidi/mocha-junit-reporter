{
  "name": "mocha-junit-reporter",
  "version": "2.0.0",
  "description": "A JUnit reporter for mocha.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint index.js test/*; exit 0",
    "test": "MOCHA_FILE=test/mocha.xml node_modules/.bin/mocha test --reporter=spec",
    "tdd": "MOCHA_FILE=test/mocha.xml node_modules/.bin/mocha test --reporter=min --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aaomidi/mocha-junit-reporter"
  },
  "keywords": [
    "mocha",
    "junit",
    "reporter"
  ],
  "author": {
    "name": "Amir Omidi",
    "email": "amir@aaomidi.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.11.7",
    "libxmljs": "^0.19.5",
    "mocha": "^5.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "strip-ansi": "^4.0.0",
    "xml": "^1.0.0"
  },
  "peerDependencies": {
    "mocha": ">=2.2.5"
  }
}
